{
  "name": "memora-graph",
  "version": "1.0.0",
  "description": "Cloudflare Pages deployment for Memora knowledge graph visualization",
  "private": true,
  "scripts": {
    "setup": "./scripts/setup-cloudflare.sh",
    "dev": "wrangler pages dev ./public --local --persist-to=.wrangler/state/v3",
    "deploy": "wrangler pages deploy ./public --project-name=memora-graph",
    "deploy:worker": "cd worker && wrangler deploy",
    "d1:create": "wrangler d1 create memora-graph",
    "d1:migrate": "wrangler d1 migrations apply memora-graph",
    "d1:migrate-local": "wrangler d1 migrations apply memora-graph --local",
    "sync": "./scripts/sync.sh",
    "sync-remote": "./scripts/sync.sh --remote"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240620.0",
    "typescript": "^5.4.5",
    "wrangler": "^3.60.0"
  }
}
